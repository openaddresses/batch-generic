{
    "ts-node": {
        "esm": true
    },
    "allowJs": true,
    "compilerOptions": {
        "noEmit": true,
        "skipLibCheck": true,
        "module": "es2022",
        "esModuleInterop": true,
        "allowImportingTsExtensions": true,
        "target": "es2022",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "."
    },
    "include": [
        "generic.ts",
        "lib/**/*"
    ]
}
